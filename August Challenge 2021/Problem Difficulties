/** Problem Difficulties **/

t=int(input())
for i in range(t):
    l=list(map(int,input().split(" ")))
    l1=list(set(l))
    if len(l1)==4:
        print("2")
    elif len(l1)==3:
        print("2")
    elif len(l1)==2:
        k=l1[0]
        if l.count(k)==2:
            print("2")
        else:
            print("1")
    else:
        print("0")
        
    
